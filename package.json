{
  "name": "",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build2": "ng build ng-cmd-builder --prod && cpx README.md ./dist/ng-cmd-builder/",
    "build": "npm run builder:build && npm run build:assets",
    "build:assets": "cpx README.md ./dist/ng-cmd-builder/ && cpx ./projects/ng-cmd-builder/builders.json ./dist/ng-cmd-builder && cpx ./projects/ng-cmd-builder/package.json ./dist/ng-cmd-builder && cpx ./projects/ng-cmd-builder/src/lib/build/schema.json ./dist/ng-cmd-builder/lib/build && cpx ./projects/ng-cmd-builder/src/lib/build/schema.d.ts ./dist/ng-cmd-builder/lib/build",
    "builder:build": "tsc -p projects/ng-cmd-builder/tsconfig.lib.prod.json",
    "test:build": "tsc -p projects/ng-cmd-builder/tsconfig.spec.json ",
    "test": "npm run test:build && node ./out-tsc/**/index.specs.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "commit-retry": "git cz --retry",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1001.2",
    "@angular-devkit/core": "10.1.2",
    "@angular-devkit/architect": "0.1001.2",
    "@angular/cli": "10.1.2",
    "@angular/common": "10.1.2",
    "@angular/compiler": "10.1.2",
    "@angular/compiler-cli": "10.1.2",
    "@angular/core": "10.1.2",
    "@angular/language-service": "10.1.2",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.39",
    "codelyzer": "5.2.2",
    "cpx": "1.5.0",
    "rxjs": "6.6.3",
    "zone.js": "0.10.3",
    "cz-conventional-changelog": "3.2.0",
    "husky": "4.2.5",
    "jasmine": "3.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "moq.ts": "6.3.1",
    "semantic-release": "17.0.7",
    "ts-node": "8.10.1",
    "tslint": "6.1.3",
    "tslib": "2.0.1",
    "typescript": "4.0.3",
    "@testdozer/ng-cmd-builder": "1.0.0"
  },
  "release": {
    "branches": [
      {
        "name": "master"
      },
      {
        "name": "dev",
        "channel": "next"
      }
    ],
    "pkgRoot": "dist/ng-cmd-builder"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint --edit"
    }
  }
}
