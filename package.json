{
  "name": "",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "build": "ng build ng-cmd-builder",
    "test": "npm run test:build && cd ./out-tsc/spec/ && node index.specs.js",
    "builder:build": "tsc -p projects/ng-cmd-builder/tsconfig.lib.prod.json",
    "test:build": "tsc -p projects/ng-cmd-builder/tsconfig.spec.json ",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "commit-retry": "git cz --retry",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/architect": "0.1202.1",
    "@angular-devkit/build-angular": "12.2.1",
    "@angular-devkit/core": "12.2.1",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "12.2.1",
    "@angular/compiler": "12.2.1",
    "@angular/compiler-cli": "12.2.1",
    "@angular/core": "12.2.1",
    "@angular/language-service": "12.2.1",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@testdozer/ng-cmd-builder": "1.2.1",
    "@types/jasmine": "3.6.3",
    "@types/node": "12.19.16",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "cpx": "1.5.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.13",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "4.3.8",
    "jasmine": "3.6.4",
    "jasmine-core": "3.6.0",
    "moq.ts": "6.3.1",
    "ng-auto-moq": "4.0.2",
    "rxjs": "6.6.3",
    "semantic-release": "17.4.5",
    "ts-node": "8.10.2",
    "tslib": "2.1.0",
    "tslint": "6.1.3",
    "typescript": "4.3.5"
  },
  "release": {
    "branches": [
      {
        "name": "master"
      },
      {
        "name": "dev",
        "channel": "next"
      }
    ],
    "pkgRoot": "dist/ng-cmd-builder"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint --edit"
    }
  },
  "dependencies": {}
}
