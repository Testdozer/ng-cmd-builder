{
  "name": "",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "build": "ng build ng-cmd-builder",
    "test": "npm run test:build && node ./specs/ng-cmd-builder/index.specs.js",
    "builder:build": "tsc -p projects/ng-cmd-builder/tsconfig.lib.prod.json",
    "test:build": "tsc -p projects/ng-cmd-builder/tsconfig.spec.json",
    "test:build:package": "ts-node --esm --eval 'const {writeFile}=require(\"fs\");writeFile(\"./specs/package.json\",`{\"type\":\"module\"}`, () => undefined)'",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "commit-retry": "git cz --retry",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/architect": "0.1402.3",
    "@angular-devkit/build-angular": "12.2.18",
    "@angular-devkit/core": "12.2.18",
    "@angular-eslint/builder": "14.1.1",
    "@angular-eslint/eslint-plugin": "14.1.1",
    "@angular-eslint/eslint-plugin-template": "14.1.1",
    "@angular-eslint/schematics": "14.1.1",
    "@angular-eslint/template-parser": "14.1.1",
    "@angular/cli": "12.2.18",
    "@angular/compiler": "12.2.16",
    "@angular/compiler-cli": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/language-service": "12.2.16",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@testdozer/ng-cmd-builder": "1.2.5",
    "@types/jasmine": "4.3.0",
    "@types/node": "16.11.59",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "cpx": "1.5.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "8.0.1",
    "jasmine": "4.4.0",
    "jasmine-core": "4.4.0",
    "moq.ts": "9.0.2",
    "ng-auto-moq": "5.2.0",
    "rxjs": "6.6.3",
    "semantic-release": "19.0.5",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "tslint": "6.1.3",
    "typescript": "4.3.5",
    "@testdozer/ng-injector-types": "1.0.2",
    "cpy": "9.0.1"
  },
  "release": {
    "branches": [
      {
        "name": "master"
      },
      {
        "name": "dev",
        "channel": "next"
      }
    ],
    "pkgRoot": "dist/ng-cmd-builder"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint --edit"
    }
  },
  "dependencies": {}
}
