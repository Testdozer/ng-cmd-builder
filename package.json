{
  "name": "",
  "version": "0.0.0-semantically-released",
  "build": {
    "target": "./dist/ng-cmd-builder/",
    "source": "./projects/ng-cmd-builder"
  },
  "scripts": {
    "build": "ng build ng-cmd-builder && npm run build:assets",
    "test": "npm run test:build && cd ./out-tsc/spec/ && node index.specs.js",
    "build:builder": "tsc -p projects/ng-cmd-builder/tsconfig.lib.prod.json",
    "build:assets": "node copy:assets.js",
    "test:build": "tsc -p projects/ng-cmd-builder/tsconfig.spec.json ",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "commit-retry": "git cz --retry",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/architect": "0.1001.2",
    "@angular-devkit/build-angular": "0.1001.2",
    "@angular-devkit/core": "10.1.2",
    "@angular/cli": "10.1.2",
    "@angular/compiler": "10.1.2",
    "@angular/compiler-cli": "10.1.2",
    "@angular/core": "10.1.2",
    "@angular/language-service": "10.1.2",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@testdozer/ng-cmd-builder": "1.0.2",
    "@types/jasmine": "3.5.11",
    "@types/node": "12.12.39",
    "codelyzer": "5.2.2",
    "cpx": "1.5.0",
    "cz-conventional-changelog": "3.2.0",
    "husky": "4.2.5",
    "jasmine": "3.6.1",
    "jasmine-core": "3.6.0",
    "moq.ts": "6.3.1",
    "rxjs": "6.6.3",
    "semantic-release": "17.0.7",
    "ts-node": "8.10.1",
    "tslib": "2.0.1",
    "tslint": "6.1.3",
    "typescript": "4.0.3"
  },
  "release": {
    "branches": [
      {
        "name": "master"
      },
      {
        "name": "dev",
        "channel": "next"
      }
    ],
    "pkgRoot": "dist/ng-cmd-builder"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint --edit"
    }
  },
  "dependencies": {}
}
